function solution(inputArray) {
  let maxlength=0;
  const result=[];
  for(let i=0;i<inputArray.length;i++){
    let string= inputArray[i];
    if(string.length>maxlength){
     maxlength=string.length;
    }
  }
  for(let i=0;i<inputArray.length;i++){
    let string= inputArray[i];
    if(string.length==maxlength){
     result.push(string);
    }
  }
  return result;
}
