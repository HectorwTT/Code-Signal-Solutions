String solution(String inputString) {
  String newString ="";
  for(int i=0;i<inputString.length();i++){
      char current = inputString.charAt(i);
      int next = current +1;
      if(next ==123 || next == 91) next -= 26; 
      char replacech = (char)next;
      newString += replacech;
  }
  return newString;
}
