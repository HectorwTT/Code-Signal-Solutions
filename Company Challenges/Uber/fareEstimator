double[] solution(int ride_time, int ride_distance, double[] cost_per_minute, double[] cost_per_mile) {
   double [] total = new double[cost_per_minute.length];
   for(int i=0;i<total.length;i++){
       total[i]= (cost_per_minute [i]* ride_time) + (ride_distance * cost_per_mile[i]);
   }
   return total;
}
